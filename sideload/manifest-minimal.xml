  <AppDomains>
    <AppDomain>http://localhost:3000</AppDomain>
  </AppDomains>

  <Hosts>
    <Host Name="Workbook" />
  </Hosts>

  <DefaultSettings>
    <SourceLocation DefaultValue="http://localhost:3000/index.html" />
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>

